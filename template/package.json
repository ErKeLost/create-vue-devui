    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0"
  },
  "devDependencies": {
    "vite": "^3.0.4",
    "sass": "^1.53.0",
    "eslint": "^8.18.0",
    "eslint-plugin-vue": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "prettier": "^2.7.1",
    "eslint-config-prettier": "^8.5.0",
